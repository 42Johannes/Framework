<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProjectType>Library</ProjectType>
  </PropertyGroup>
  <Import Project="..\..\..\Build\Shared.build.props" />
  <PropertyGroup>
    <AssemblyTitle>re-motion Development Support Library for RhinoMocks</AssemblyTitle>
    <Description>Library for unit testing with RhinoMocks.</Description>
    <AssemblyName>Remotion.Development.RhinoMocks</AssemblyName>
    <RootNamespace>$(AssemblyName)</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\..\..\SharedSource\Core\FunctionalProgramming\*.cs" Link="SharedSource\FunctionalProgramming\%(Filename)%(Extension)" />
    <Compile Include="..\..\..\SharedSource\Core\Utilities\*.cs" Link="SharedSource\Utilities\%(Filename)%(Extension).cs" />
    <Compile Include="..\..\..\SharedSource\Development\RhinoMocks\UnitTesting\**\*.cs" Link="SharedSource\RhinoMocks\UnitTesting\%(RecursiveDir)%(Filename)%(Extension).cs" />
    <Compile Include="..\..\..\SharedSource\Core\RoslynAnnotations\NullableAnnotations.cs" Link="SharedSource\RoslynAnnotations\NullableAnnotations.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CoreRhinoMocks" Version="3.6.1.2" />
    <PackageReference Include="NUnit" Version="3.12.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Core\Development.Core.csproj" />
  </ItemGroup>
  <Import Project="..\..\..\Build\Shared.build.targets" />
</Project>